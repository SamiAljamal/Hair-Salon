<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
  <title></title>
  <title></title>
</head>
<body>
  <div class="jumbotron">
    @if (@Model.Count == 0)
    {
      <p>No Stylists have been added yet!</p>
    }
    else
    {
      <h1>Here is your list of stylists:</h1>
      <ul>
        @foreach (var item in Model)
        {
          <li><a href="/stylist/@item.GetId()">@item.GetName()</a>| <a href="/stylist/edit/@item.GetId()">Edit</a>| <a href="/cuisines/delete/@item.GetId()">Delete</a></li>
        }
      </ul>
    }


    </div>

  </body>
  </html>
